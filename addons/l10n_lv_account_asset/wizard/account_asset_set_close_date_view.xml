<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="account_asset_set_close_date_form" model="ir.ui.view">
            <field name="name">Set Date Closed for Account Asset</field>
            <field name="model">account.asset.set.close.date</field>
            <field name="arch" type="xml">
                <form string="Set Date Closed for Account Asset" version="7.0">
                    <label string="Choose Date." colspan="4"/>
                    <separator string="" colspan="4"/>
					<group col="2">
                    	<field name="close_date"/>
					</group>
                    <separator string="" colspan="4"/>
                    <footer>
                        <button name="set_close_date" string="Set" type="object" default_focus="1" class="oe_highlight"/>
                        <button special="cancel" string="Cancel" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>
        
    </data>
</openerp>
