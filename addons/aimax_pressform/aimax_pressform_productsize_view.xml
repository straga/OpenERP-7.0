            <field name="model">ampf.productsize</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Product Size">
                    <separator string="Product Size" colspan="4"/>
                    <field colspan="4" name="name"/>
                    <separator string="WxHxH" colspan="4"/>
                    <field name="ampf_productsize_diametr"/>
                    <separator string="WxHxH" colspan="4"/>
                    <field name="ampf_productsize_width"/>
